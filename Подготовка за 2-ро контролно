#include <iostream>
using namespace std;

void newNum(int num, int digit) {
	int lastDigit;
	int counter = 1;
	int newnumber = 0;
	while (num != 0) {
		lastDigit = (num % 10 + digit) % 10;
		newnumber += lastDigit * counter;
		counter *= 10; num /= 10;
	}
	cout << newnumber;
}

int main()
{
	int num, digit;
	cin >> num >> digit;
	newNum(num, digit);
	return 0;
}

