#include <iostream>
using namespace std;

void remove_digit(int arr[128], int size, int digit) {
	for (int i = 0; i < size; i++) {
		if (arr[i] == digit) {
			for (int j = i; j < size - 1; j++) {
				arr[j] = arr[j + 1];
			}
			i--; size--;
		}
	}
	for (int i = 0; i < size; i++) {
		cout << arr[i] << " ";
	}
}

int main()
{
	int arr[128], size, digit;
	cin >> size;
	for (int i = 0; i < size; i++) {
		cin >> arr[i];
	}
	cin >> digit;
	remove_digit(arr, size, digit);
	return 0;
}

/////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

#include <iostream>
using namespace std;

int sum_of_four_diag() {
	int arr[32][32], size;
	bool check = false;
	int sum_1_diag = 0, sum_2_diag = 0, sum_3_diag = 0, sum_4_diag = 0;
	cin >> size;
	if (size % 2 == 0 || size <= 3) { return check; }
	for (int i = 0; i < size; i++) {
		for (int j = 0; j < size; j++) {
			cin >> arr[i][j];
		}
	}
	for (int i = 0; i < size / 2; i++) {
		sum_1_diag += arr[i][i];
	}

	for (int i = 0; i < size / 2; i++) {
		sum_2_diag += arr[i][size / 2 + 1 + i];
	}
	
	for (int i = 0; i < size / 2; i++) {
		sum_3_diag += arr[size / 2 + 1 + i][i];
	}

	for (int i = 0; i < size / 2; i++) {
		sum_4_diag += arr[size / 2 + 1 + i][size / 2 + 1 + i];
	}
	
	check = (sum_1_diag == sum_2_diag && sum_2_diag == sum_3_diag && sum_3_diag == sum_4_diag);
	return check;
}

int main(){
	cout << (sum_of_four_diag() ? "True" : "False");
	return 0;
}

