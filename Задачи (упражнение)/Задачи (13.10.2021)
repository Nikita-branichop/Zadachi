//Task 1//////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
    int four_digit_number;
    cin >> four_digit_number;
    int first_digit = four_digit_number / 1000;
    int second_digit = four_digit_number / 100 % 10;
    int third_digit = four_digit_number / 10 % 10;
    int fourth_digit = four_digit_number % 10;
    cout << "first digit: " << first_digit << endl;
    cout << "second digit: " << second_digit << endl;
    cout << "third digit: " << third_digit << endl;
    cout << "fourth digit: " << fourth_digit << endl;
    cout << "sum: " << first_digit + second_digit + third_digit + fourth_digit;
    return 0;
}
 
//Task 2//////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
        int four_digit_number;
        cin >> four_digit_number;
        int first_digit = four_digit_number / 1000;
        int second_digit = four_digit_number / 100 % 10;
        int third_digit = four_digit_number / 10 % 10;
        int fourth_digit = four_digit_number % 10;
        int new_digit = fourth_digit * 1000 + third_digit * 100 + second_digit * 10 + first_digit;
        cout << new_digit;
    return 0;
}
 
//Task 3///////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
    int fst_num, sec_num;
    cin >> fst_num >> sec_num;
    int multi = fst_num * sec_num;
    int last_digit_of_multi = multi % 10;
    bool x = (last_digit_of_multi % 2 == 0);
    cout << multi << ", " << last_digit_of_multi << " - " << x;
    return 0;
}
 
//Task 4///////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
    int a, b;
    cout << "a = "; cin >> a;
    cout << "b = "; cin >> b;
    a = a + b;
    b = a - b;
    a = a - b;
    cout << "a = " << a << " , " << "b = " << b;
    return 0;
}
 
//Task 4.1////////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
    int a, b, additional;
    cout << "a = "; cin >> a;
    cout << "b = "; cin >> b;
    additional = a;
    a = b;
    b = additional;
    cout << "a = " << a << " , " << "b = " << b;
    return 0;
}
 
//Task 5///////////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
    int fst_digit, sec_digit;
    cin >> fst_digit >> sec_digit;
    bool check = (fst_digit % sec_digit == 0);
    cout << boolalpha << check;
    return 0;
}
 
//Taks 6///////////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
 
    int a, b, c;
    cin >> a >> b >> c;
    bool check = (a + b > c && a + c > b && b + c > a)
    cout << boolalpha << check;
    return 0;
}
 
//Task 7////////////////////////////////////////
 
int main()
{
    int a, b, c;
    cin >> a >> b >> c;
    cout << "Perimeter = " << a + b + c << endl;
    int semi_perimeter = (a + b + c) / 2;
    cout << "Area = " << sqrt(semi_perimeter * (semi_perimeter - a) * (semi_perimeter - b) * (semi_perimeter - c));
    return 0;
}
 
//Task 8/////////////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
    double first_digit, second_digit, third_digit;
    cin >> first_digit >> second_digit >> third_digit;
    if (first_digit > second_digit && first_digit > third_digit) {
        if (second_digit > third_digit) {
            cout << first_digit << " " << third_digit;
        }
        else cout << first_digit << " " << second_digit;
    } else if (second_digit > first_digit && second_digit > third_digit) {
        if (first_digit > third_digit) {
            cout << second_digit << " " << third_digit;
        }
        else cout << second_digit << " " << first_digit;
    }
    else if (third_digit > first_digit && third_digit > second_digit) {
        if (first_digit > second_digit) {
            cout << third_digit << " " << second_digit;
        }
        else cout << third_digit << " " << first_digit;
    }
    else cout << "Wrong";
    return 0;
}
 
//Task 9////////////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
    int x, a, b;
    cin >> x >> a >> b;
    bool check = (a < x && b > x);
    cout << boolalpha << check;
    return 0;
}
 
//Task 10/////////////////////////////////////////
 
#include <iostream>
using namespace std;
int main()
{
    int year;
    cin >> year;
    if (year % 4 == 0 && year % 400 == 0) {
        cout << year << " is a leap year";
    }
    else cout << year << " is not a leap year";
    return 0;
}
 
