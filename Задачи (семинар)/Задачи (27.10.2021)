//Zadacha 1////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int number, sum = 0;
    cin >> number;
    while (number != 0){
        cin >> number;
        sum += number;
    }
    cout << sum;
    return 0;
}

//Zadacha 2///////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int number, sum = 0;
    cin >> number;
    while (number != 0) {
        int digit = number % 10;
        number /= 10;
        sum += digit;
    }
    cout << sum;
    return 0;
}

//Zadacha 3/////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int number;
    cin >> number;
    for (int i = 1; i <= number; i++) {
        cout << i << " ";
    }
    return 0;
}

//Zadacha 4///////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int number, sum = 1;
    cin >> number;
    for (int i = number; i >= 1; i--) {
        sum = sum * i;
    }
    cout << sum;
    return 0;
}

//Zadacha 5/////////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int a, n, sum = 1;
    cin >> a >> n;
    for (int i = 0; i < n; i++) {
        sum *= a;
    }
    cout << sum;
    return 0;
}

//Zadacha 6///////////////////////////////////////////////////////////

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int a, n, sum = 0;
    cin >> a >> n;
    for (int i = 0; i <= n; i++) {
        sum = sum + pow(a, i);
    }
    cout << sum;
    return 0;
}

//Zadacha 7/////////////////////////////////////////////////////////////

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int n;
    cin >> n;
    n = sqrt(n);
    for (int i = 1; i <= n; i++) {
        cout << i * i << " ";
    }
    return 0;
}

//Zadacha 8//////////////////////////////////////////////////////////////

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int numberDigits, check = 0;
    int digit;
    cout << "Count of number - "; cin >> numberDigits;
    for (int i = 1; i <= numberDigits; i++) {
        cin >> digit;
        if (digit % 2 == 1) {
            check += 1;
        }
    }
    bool(check == numberDigits) ? (cout << "YES") : (cout << "NO");
    return 0;
}

//Zadacha 9////////////////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int number, sum = 0;
    int check = 0;
    cin >> number;
    while (number != 0) {
        int digit = number % 10;
        number /= 10;
        if (digit == 1) {
            check += 1;
        }
    }
    bool(check >= 1) ? (cout << "YES") : (cout << "NO");
    return 0;
}

//Zadacha 10///////////////////////////////////////////////////////////////////

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int number;
    cin >> number;
    for (int i = 2; i < sqrt(number); i++) {
        if (number % i == 0) {
            cout << "Chisloto ne e prosto";
            return 0;
        }
    }
    cout << "Chisloto e prosto";
    return 0;
}

//Zadacha 11///////////////////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int number;
    cin >> number;
    for (int i = 2; i < number; i++) {
        for (int i1 = 2; i1 * i1 <= i; i1++)
        {
            if (i % i1 == 0)
                break;
            else if (i1 + 1 > sqrt(i)) {
                cout << i << " ";
            }
        }
    }
    return 0;
}

//Zadacha 12///////////////////////////////////////////////////////////////////

#include <iostream>
    using namespace std;
    int main()
    {
        int number, i2 = 0;
        cin >> number;
            for (int i = 0; i2 < number; i++) {
                for (int i1 = 2; i1 * i1 <= i; i1++)
                {
                    if (i % i1 == 0)
                        break;
                    else if (i1 + 1 > sqrt(i)) {
                        cout << i << " "; i2++;
                    }
                }
            }
        return 0;
    }
