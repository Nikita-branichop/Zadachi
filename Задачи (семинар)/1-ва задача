//Zadacha 1a ////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    double x, y, r;
    bool flag = false;
    cin >> x >> y;
    if ((x >= 2 && y <= 0) && (x - 4) * (x - 4) + y * y <= 4) {
    flag = true;
    }
    if ((y >= 0) && x * x + y * y <= 4) {
        flag = true;
    }
    if ((x <= -2 && y <= 0) && (x + 4) * (x + 4) / 4 + y * y / 9 <= 1) {
        flag = true;
    }
    cout << (flag ? "YES" : "NO");
    return 0;
}

//Zadacha 1b /////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    double x, y, r;
    bool flag = false;
    cin >> x >> y;
    if (y >= 0 && y <= 4 && x >= -3 && x <= 3) {
        flag = true;
        if (x * x + y * y < 9) {
            flag = false;
        }
    }
    cout << (flag ? "YES" : "NO");
    return 0;
}

//Zadacha 1c //////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    double x, y, r;
    bool flag = false;
    cin >> x >> y;
    if (y >= -3 && y <= 2 && x <= 3 && x >= -3) {
        flag = true;
    }
    if (x * x / 9 + (y - 2) * (y - 2) / 4 <= 1) {
        flag = true;
    }
    cout << (flag ? "YES" : "NO");
    return 0;
}

//Zadacha 2 ///////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int num;
    cin >> num;
    if (num > 2) {
        for (int i = 1; i <= num * 2 + num / 2; i++) {
            if (i <= num) {
                cout << "*";
            }
            if (i > num + num / 2) {
                cout << "*";
            }
            if (i > num && i < num + num / 2 + 1) {
                cout << " ";
            }
        }
        cout << endl;
        for (int j = 1; j <= num / 2; j++) {
            for (int j1 = 1; j1 <= num * 2 + num / 2 - 1; j1++) {
                if (j1 == 1 || (j1 >= num && j1 <= num + num / 2 + 1)) {
                    cout << "*";
                }
                else cout << " ";
                if (j1 == 2 * num + num / 2 - 1) {
                    cout << "*" << endl;
                }
            }
        }
        for (int i = 1; i <= num * 2 + num / 2; i++) {
            if (i <= num) {
                cout << "*";
            }
            if (i > num + num / 2) {
                cout << "*";
            }
            if (i > num && i < num + num / 2 + 1) {
                cout << " ";
            }
        }
    }
    else cout << "Wrong number";
    return 0;
}

//Zadacha 3 /////////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int num;
    cin >> num;
    int lenght = num * 3 - 2;
    int temp = lenght;
    if (num > 1) {
        for (int i = 1; i < num; i++) {
            for (int i1 = 1; i1 <= i; i1++) {
                cout << i1;
            }
            for (int i2 = 1; i2 <= temp; i2++) {
                cout << " ";
            }
            for (int i3 = i; i3 >= 1; i3--) {
                cout << i3;
            }
            temp = temp - 2;
            cout << endl;
        }
        temp += 2;
        for (int k = 1; k <= num * 3; k++) {
            if (k <= num) {
                cout << k;
            }
            if (k > num && k <= num * 2) {
                cout << "-";
            }
        }
        for (int k1 = num; k1 >= 1; k1--) {
            cout << k1;
        }
        cout << endl;
        for (int j = num; j > 1; j--) {
            for (int j1 = 1; j1 < j; j1++) {
                cout << j1;
            }
            for (int j2 = 1; j2 <= temp; j2++) {
                cout << " ";
            }
            temp += 2;
            for (int j3 = j - 1; j3 >= 1; j3--) {
                cout << j3;
            }
            cout << endl;
        }
    }
    else cout << "Wrong number";
    return 0;
}

//Zadacha 4///////////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int num;
    cin >> num;
    if (num > 0) {
        for (int i = 0; i < num + 1; i++) {
            for (int i1 = 0; i1 <= 2 * num + 1; i1++) {
                if (i1 >= i && i1 < num * 2 + 1 - i) {
                    cout << "*";
                }
                else cout << " ";
            }
            cout << endl;
        }
        for (int j = 1; j <= num; j++) {
            for (int j1 = 1; j1 <= 2 * num + 1; j1++) {
                if (j1 > num - j && j1 < num + j + 2) {
                    cout << "*";
                }
                else cout << " ";
            }
            cout << endl;
        }
    }
    else cout << "Wrong number";
    return 0;
}

//Zadacha 5//////////////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int num;
    cin >> num;
    if (num > 2) {
        for (int j = 1; j <= num; j++) {
            for (int j1 = 1; j1 <= 2 * num + 1; j1++) {
                if (j1 > num - j + 1 && j1 < num + j + 1) {
                    cout << "*";
                }
                else cout << " ";
            }
            cout << endl;
        }
        for (int i = 1; i <= num / 2; i++) {
            for (int i1 = 1; i1 <= num + 1; i1++) {
                if (i1 == num + 1) {
                    cout << "*";
                }
                else cout << " ";
            }
            cout << endl;
        }
    }
    else cout << "Wrong number";
    return 0;
}

//Zadacha 6/////////////////////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int a, b, sum = 0, multiply = 1, check = 0, temp;
    cin >> a >> b;
    for (int i = a; i <= b; i++) {
        temp = i;
        while (temp != 0) {
            int digit = temp % 10;
            temp /= 10;
            sum += digit;
            multiply *= digit;
        }
            if (sum == multiply) {
                check++;
            }
            sum = 0;
            multiply = 1;
    }
    cout << check;
    return 0;
}

//Zadacha 7///////////////////////////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int n, k, x = 0, temp;
    cin >> n >> k;
    temp = k;
    while (temp <= n) {
        temp *= k;
        x++;
    }
    cout << x;
    return 0;
}

//Zadacha 8///////////////////////////////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int n, k, digit1, digit2, temp_k, temp_n, num_digits_k = 0, num_overlap = 0;
    bool check = false;
    cin >> n;
    cin >> k;
    temp_k = k;
    temp_n = n;
    if (k != 0) {
        while (temp_k != 0) {
            temp_k /= 10; num_digits_k++;
        }
        temp_k = k;
        while (temp_n != 0){
            digit1 = temp_n % 10;
            temp_n /= 10;
            digit2 = temp_k % 10;
            if (digit1 == digit2) {
                num_overlap++;
                temp_k /= 10;
                if (num_overlap == num_digits_k) {
                    check = true;
                }
            }
            else {
                if (num_overlap != 0) temp_n = temp_n * 10 + digit1;
                temp_k = k; num_overlap = 0;
            }
        }
    }
    cout << boolalpha << check;
    return 0;
}
