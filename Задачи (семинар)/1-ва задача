//Zadacha 1a ////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    double x, y, r;
    bool flag = false;
    cin >> x >> y;
    if ((x >= 2 && y <= 0) && (x - 4) * (x - 4) + y * y <= 4) {
    flag = true;
    }
    if ((y >= 0) && x * x + y * y <= 4) {
        flag = true;
    }
    if ((x <= -2 && y <= 0) && (x + 4) * (x + 4) / 4 + y * y / 9 <= 1) {
        flag = true;
    }
    cout << (flag ? "YES" : "NO");
    return 0;
}

//Zadacha 1b /////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    double x, y, r;
    bool flag = false;
    cin >> x >> y;
    if (y >= 0 && y <= 4 && x >= -3 && x <= 3) {
        flag = true;
        if (x * x + y * y < 9) {
            flag = false;
        }
    }
    cout << (flag ? "YES" : "NO");
    return 0;
}

//Zadacha 1c //////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    double x, y, r;
    bool flag = false;
    cin >> x >> y;
    if (y >= -3 && y <= 2 && x <= 3 && x >= -3) {
        flag = true;
    }
    if (x * x / 9 + (y - 2) * (y - 2) / 4 <= 1) {
        flag = true;
    }
    cout << (flag ? "YES" : "NO");
    return 0;
}

//Zadacha 2 ///////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int num;
    cin >> num;
    if (num > 2) {
        for (int i = 1; i <= num * 2 + num / 2; i++) {
            if (i <= num) {
                cout << "*";
            }
            if (i > num + num / 2) {
                cout << "*";
            }
            if (i > num && i < num + num / 2 + 1) {
                cout << " ";
            }
        }
        cout << endl;
        for (int j = 1; j <= num / 2; j++) {
            for (int j1 = 1; j1 <= num * 2 + num / 2 - 1; j1++) {
                if (j1 == 1 || (j1 >= num && j1 <= num + num / 2 + 1)) {
                    cout << "*";
                }
                else cout << " ";
                if (j1 == 2 * num + num / 2 - 1) {
                    cout << "*" << endl;
                }
            }
        }
        for (int i = 1; i <= num * 2 + num / 2; i++) {
            if (i <= num) {
                cout << "*";
            }
            if (i > num + num / 2) {
                cout << "*";
            }
            if (i > num && i < num + num / 2 + 1) {
                cout << " ";
            }
        }
    }
    else cout << "Wrong number";
    return 0;
}
