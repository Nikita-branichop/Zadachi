//Zadacha 1a ////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    double x, y, r;
    bool flag = false;
    cin >> x >> y;
    if ((x >= 2 && y <= 0) && (x - 4) * (x - 4) + y * y <= 4) {
    flag = true;
    }
    if ((y >= 0) && x * x + y * y <= 4) {
        flag = true;
    }
    if ((x <= -2 && y <= 0) && (x + 4) * (x + 4) / 4 + y * y / 9 <= 1) {
        flag = true;
    }
    cout << (flag ? "YES" : "NO");
    return 0;
}

//Zadacha 1b /////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    double x, y, r;
    bool flag = false;
    cin >> x >> y;
    if (y >= 0 && y <= 4 && x >= -3 && x <= 3) {
        flag = true;
        if (x * x + y * y < 9) {
            flag = false;
        }
    }
    cout << (flag ? "YES" : "NO");
    return 0;
}

//Zadacha 1c //////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    double x, y, r;
    bool flag = false;
    cin >> x >> y;
    if (y >= -3 && y <= 2 && x <= 3 && x >= -3) {
        flag = true;
    }
    if (x * x / 9 + (y - 2) * (y - 2) / 4 <= 1) {
        flag = true;
    }
    cout << (flag ? "YES" : "NO");
    return 0;
}
