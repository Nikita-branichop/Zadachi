//Zadacha 1///////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int digit;
    cin >> digit;
    if (digit % 2 == 0) {
        cout << "Even number";
    }
    else cout << "Odd number";
    return 0;
}

//Zadacha 2////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int year;
    cin >> year;
    if (year % 4 == 0 && year % 400 == 0) {
        cout << "This year is a leap";
    }
    else cout << "This year is a not leap";
    return 0;
}

//Zadacha 3//////////////////////////////////////////////

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int a, b, c;
    cout << "a = "; cin >> a;
    cout << "b = "; cin >> b;
    cout << "c = "; cin >> c;
    int x1 = -b + sqrt(pow(b, 2))
    return 0;
}
