//Zadacha 1///////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int digit;
    cin >> digit;
    if (digit % 2 == 0) {
        cout << "Even number";
    }
    else cout << "Odd number";
    return 0;
}

//Zadacha 2////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int year;
    cin >> year;
    if ((year % 100 != 0 && year % 4 == 0) || year % 400 == 0 ) {
        cout << year << " is a leap year";
    }
    else cout << year << " is not a leap year";
    return 0;
}

//Zadacha 3//////////////////////////////////////////////

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int a, b, c;
    cout << "a = "; cin >> a;
    cout << "b = "; cin >> b;
    cout << "c = "; cin >> c;
    int x1 = (-b + sqrt(pow(b, 2) - 4 * a * c)) / 2 * a;
    int x2 = (-b - sqrt(pow(b, 2) - 4 * a * c)) / 2 * a;
    cout << "x1 = " << x1 << endl;
    cout << "x2 = " << x2 << endl;
    return 0;
}

//Zadacha 4///////////////////////////////////////////////

#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    double first_digit, second_digit;
    char operation;
    cout << "First digit = "; cin >> first_digit;
    cout << "Second digit = "; cin >> second_digit;
    cout << "Operation (+ , - , * , / ) "; cin >> operation;
    switch (operation) {
    case '+': cout << first_digit + second_digit; break;
    case '-': cout << first_digit - second_digit; break;
    case '*': cout << first_digit * second_digit; break;
    case '/': cout << first_digit / second_digit; break;
    default: cout << "Wrong"; break;
    }
    return 0;
}

//Zadacha 5///////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    char symbol;
    int decimal;
    cin >> symbol;
    decimal = int(symbol);
    if (decimal >= 97 && decimal <= 122) {
        cout << "The upper case character corresponding to is ...";
    }
    else if (decimal >= 65 && decimal <= 90) {
        cout << "The lower case character corresponding to is ...";
    }
    else cout << "is not a letter";
    return 0;
}

//Zadacha 6////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    char symbol;
    cin >> symbol;
    int decimal; 
    decimal = int(symbol);
    if (decimal >= 97 && decimal <= 122) {
        decimal = decimal - 32; cout << char(decimal);
    }
    else if (decimal >= 65 && decimal <= 90) {
        decimal = decimal + 32; cout << char(decimal);
    }
    else if (decimal >= 48 && decimal <= 57) {
        int digit = decimal - '0';
        cout << digit * digit;
    }
    else cout << "Wrong";
    return 0;
}

//Zadacha 7////////////////////////////////////////////////////

#include <iostream>
using namespace std;
int main()
{
    int glue, slats, paint;
    int product;
    cout << "Glue = "; cin >> glue;
    cout << "Slats = "; cin >> slats;
    cout << "Paint = "; cin >> paint;
    paint = paint * 1000;
    const int need_glue = 10, need_slats = 4, need_paint = 20;
    int use_glue = glue / need_glue;
    int use_slats = slats / need_slats;
    int use_paint = paint / need_paint;
    int min = use_glue;
    if (use_slats < min) {
        min = use_slats;
    }
    if (use_paint < min) {
        min = use_paint;
    }
    int remainder_glue = glue - need_glue * min;
    int remainder_slats = slats - need_slats * min;
    int remainder_paint = paint - need_paint * min;
    bool check = ((remainder_glue >= 3) && (remainder_slats >= 2) && (remainder_paint >= 7));
    cout << "We can make " << min << " product/s" << endl;
    if (check == 1) {
        cout << "And we can make a toy";
    }
    else cout << "And we cannot make a toy";
    return 0;
}
